# Example configuration for API tests
# Copy this file to .env and modify values as needed

# API Test Configuration
# Base URL for the Compute API server
API_BASE_URL=http://localhost:8080

# Authentication token for API requests
# Leave empty to test unauthenticated requests
API_AUTH_TOKEN=your-test-token-here

# Test timeout settings (in seconds)
REQUEST_TIMEOUT=30
TEST_TIMEOUT=60

# Test data configuration
# Use real IDs from your test environment
TEST_ORG_ID=test-org-12345
TEST_PROJECT_ID=test-project-67890
TEST_REGION_ID=test-region-abc123
TEST_FLAVOR_ID=test-flavor-def456

# Optional: Skip integration tests if server not available
SKIP_INTEGRATION_TESTS=false

# Debug settings
ENABLE_DEBUG_LOGGING=false
LOG_HTTP_REQUESTS=true
LOG_HTTP_RESPONSES=false